package com.pack_1;

public class SwitchStatement {

	public static void main(String[] args) {
		int n = 5;
		switch(n)
		{
		case 1 :
			System.out.println("case_1");
			break;
		case 2 :
			System.out.println("case_2");
			break;
		case 3 :
			System.out.println("case_3");
			break;
		case 4 :
			System.out.println("case_4");
			break;
			default :
				System.out.println("inavlid output");
			
		}

	}

}
